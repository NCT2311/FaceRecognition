doctype html
head
  // Required meta tags
  meta(charset='utf-8')
  meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
  title Face Recognition
  // plugins:css
  link(rel='stylesheet', href='vendors/feather/feather.css')
  link(rel='stylesheet', href='vendors/ti-icons/css/themify-icons.css')
  link(rel='stylesheet', href='vendors/css/vendor.bundle.base.css')
  // endinject
  // Plugin css for this page
  link(rel='stylesheet', href='vendors/datatables.net-bs4/dataTables.bootstrap4.css')
  link(rel='stylesheet', href='vendors/ti-icons/css/themify-icons.css')
  link(rel='stylesheet', type='text/css', href='js/select.dataTables.min.css')
  // End plugin css for this page
  // inject:css
  link(rel='stylesheet', href='css/vertical-layout-light/style.css')
  // endinject
  link(rel='shortcut icon', href='images/favicon.png')
  block style
.container-scroller
  // partial:partials/_navbar.html
  nav.navbar.col-lg-12.col-12.p-0.fixed-top.d-flex.flex-row
    .text-center.navbar-brand-wrapper.d-flex.align-items-center.justify-content-center
      a.navbar-light.brand-logo.mr-5(href='home')
        img(src='assets/img/logo1.png', alt='logo')
      a.navbar-brand.brand-logo-mini(href='home')
        img(src='images/logo-mini.svg', alt='logo')
    .navbar-menu-wrapper.d-flex.align-items-center.justify-content-end
      button.navbar-toggler.navbar-toggler.align-self-center(type='button', data-toggle='minimize')
        span.icon-menu

      ul.navbar-nav.navbar-nav-right

        li.nav-item.nav-profile.dropdown
          a#profileDropdown.nav-link.dropdown-toggle(href='#', data-toggle='dropdown')
            img(src='images/faces/face28.jpg', alt='profile')
          .dropdown-menu.dropdown-menu-right.navbar-dropdown(aria-labelledby='profileDropdown')
            a.dropdown-item
              i.ti-settings.text-primary
              |                 Settings
            a.dropdown-item(href='logout')
              i.ti-power-off.text-primary
              |                 Logout
        li.nav-item.nav-settings.d-none.d-lg-flex
          a.nav-link(href='#')
            i.icon-ellipsis
      button.navbar-toggler.navbar-toggler-right.d-lg-none.align-self-center(type='button', data-toggle='offcanvas')
        span.icon-menu
  // partial
  .container-fluid.page-body-wrapper
    // partial:partials/_settings-panel.html
    .theme-setting-wrapper
      #settings-trigger
        i.ti-settings
      #theme-settings.settings-panel
        i.settings-close.ti-close
        p.settings-heading SIDEBAR SKINS
        #sidebar-light-theme.sidebar-bg-options.selected
          .img-ss.rounded-circle.bg-light.border.mr-3
          | Light
        #sidebar-dark-theme.sidebar-bg-options
          .img-ss.rounded-circle.bg-dark.border.mr-3
          | Dark
        p.settings-heading.mt-2 HEADER SKINS
        .color-tiles.mx-0.px-4
          .tiles.success
          .tiles.warning
          .tiles.danger
          .tiles.info
          .tiles.dark
          .tiles.default
    
        // To do section tab ends
        #chats-section.tab-pane.fade(role='tabpanel', aria-labelledby='chats-section')
          .d-flex.align-items-center.justify-content-between.border-bottom
            p.settings-heading.border-top-0.mb-3.pl-3.pt-0.border-bottom-0.pb-0 Friends
            small.settings-heading.border-top-0.mb-3.pt-0.border-bottom-0.pb-0.pr-3.font-weight-normal See All
          ul.chat-list
            li.list.active
              .profile
                img(src='images/faces/face1.jpg', alt='image')
                span.online
              .info
                p Thomas Douglas
                p Available
              small.text-muted.my-auto 19 min
            li.list
              .profile
                img(src='images/faces/face2.jpg', alt='image')
                span.offline
              .info
                .wrapper.d-flex
                  p Catherine
                p Away
              .badge.badge-success.badge-pill.my-auto.mx-2 4
              small.text-muted.my-auto 23 min
            li.list
              .profile
                img(src='images/faces/face3.jpg', alt='image')
                span.online
              .info
                p Daniel Russell
                p Available
              small.text-muted.my-auto 14 min
            li.list
              .profile
                img(src='images/faces/face4.jpg', alt='image')
                span.offline
              .info
                p James Richardson
                p Away
              small.text-muted.my-auto 2 min
            li.list
              .profile
                img(src='images/faces/face5.jpg', alt='image')
                span.online
              .info
                p Madeline Kennedy
                p Available
              small.text-muted.my-auto 5 min
            li.list
              .profile
                img(src='images/faces/face6.jpg', alt='image')
                span.online
              .info
                p Sarah Graves
                p Available
              small.text-muted.my-auto 47 min
        // chat tab ends
    // partial
    // partial:partials/_sidebar.html
    nav#sidebar.sidebar.sidebar-offcanvas
        ul.nav
            li.nav-item
                a.nav-link(href='/admin')
                    i.icon-camera.menu-icon
                    span.menu-title Confirm strangers
            li.nav-item
                a.nav-link(href='/user')
                    i.ti-user.menu-icon
                    span.menu-title User infomation
            li.nav-item
                a.nav-link(href='/history')
                    i.ti-search.menu-icon
                    span.menu-title Acquaintance History
            li.nav-item
                a.nav-link(href='/alldata')
                    i.ti-search.menu-icon
                    span.menu-title All history                   


    .main-panel
      .content-wrapper(style='padding-bottom:0px;padding-top:25px;')
        block content 
      footer.footer
          .d-sm-flex.justify-content-center.justify-content-sm-between
              span.text-muted.text-center.text-sm-left.d-block.d-sm-inline-block
